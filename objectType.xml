            <name>dev.daries.rp.raid.event.notification</name>
            <definitionname>com.woltlab.wcf.notification.objectType</definitionname>
            <classname>rp\system\user\notification\object\type\EventRaidUserNotificationObjectType</classname>
            <category>dev.daries.rp.event.raid</category>
        </type>
        <!-- /notification -->
        
        <!-- searchable object type -->
		<type>
			<name>dev.daries.rp.event</name>
			<definitionname>com.woltlab.wcf.searchableObjectType</definitionname>
			<classname>rp\system\search\EventSearch</classname>
			<searchindex>rp1_event_search_index</searchindex>
		</type>
        <!-- /searchable object type -->
        
		<!-- sitemap -->
		<type>
			<name>dev.daries.rp.sitemap.object.character</name>
			<definitionname>com.woltlab.wcf.sitemap.object</definitionname>
			<classname>rp\system\sitemap\object\CharacteritemapObject</classname>
			<priority>0.5</priority>
			<changeFreq>monthly</changeFreq>
			<rebuildTime>259200</rebuildTime>
		</type>
		<type>
			<name>dev.daries.rp.sitemap.object.event</name>
			<definitionname>com.woltlab.wcf.sitemap.object</definitionname>
			<classname>rp\system\sitemap\object\EventSitemapObject</classname>
			<priority>0.5</priority>
			<changeFreq>weekly</changeFreq>
			<rebuildTime>86400</rebuildTime>
		</type>
		<!-- /sitemap -->

