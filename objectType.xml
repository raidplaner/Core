        <type>
            <name>dev.daries.rp.event.comment.response</name>
            <definitionname>com.woltlab.wcf.importer</definitionname>
            <classname>rp\system\importer\EventCommentResponseImporter</classname>
        </type>
        <type>
            <name>dev.daries.rp.event.legend</name>
            <definitionname>com.woltlab.wcf.importer</definitionname>
            <classname>rp\system\importer\EventLegendImporter</classname>
        </type>
        <type>
            <name>dev.daries.rp.point.account</name>
            <definitionname>com.woltlab.wcf.importer</definitionname>
            <classname>rp\system\importer\PointAccountImporter</classname>
        </type>
        <type>
            <name>dev.daries.rp.raid</name>
            <definitionname>com.woltlab.wcf.importer</definitionname>
            <classname>rp\system\importer\RaidImporter</classname>
        </type>
        <type>
            <name>dev.daries.rp.raid.event</name>
            <definitionname>com.woltlab.wcf.importer</definitionname>
            <classname>rp\system\importer\RaidEventImporter</classname>
        </type>
