		<type>
			<name>dev.daries.rp.character</name>
			<definitionname>com.woltlab.wcf.rebuildData</definitionname>
			<classname>rp\system\worker\CharacterRebuildDataWorker</classname>
		</type>
		<type>
			<name>dev.daries.rp.event</name>
			<definitionname>com.woltlab.wcf.rebuildData</definitionname>
			<classname>rp\system\worker\EventRebuildDataWorker</classname>
		</type>
		<!-- /rebuild data workers -->
        
        <!-- Visit Tracker -->
        <type>
            <name>dev.daries.rp.event</name>
            <definitionname>com.woltlab.wcf.visitTracker.objectType</definitionname>
        </type>
        <!-- /Visit Tracker -->
